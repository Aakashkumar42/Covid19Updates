<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.coronavirus_updates">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="android.support.v4.app.CoreComponentFactory"
        android:icon="@drawable/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:replace="android:appComponentFactory">
        <activity android:name=".Acivities.IndiaDetailsActivity"></activity>
        <activity android:name=".Acivities.IndiaWiseCase" />
        <activity android:name=".Acivities.CountryDetails" />
        <activity android:name=".Acivities.AffectedCountries" />
        <activity android:name=".Acivities.GoogleWithSignInActivity" />
        <activity android:name=".Acivities.VerifyPhoneActivity" />
        <activity android:name=".Acivities.SendOTPActivity" />
        <activity android:name=".Acivities.LoginActivity" />
        <activity android:name=".Acivities.SignUpActivity" />
        <activity android:name=".Acivities.helplineNoActivity" />
        <activity android:name=".Acivities.DashBroadActivity" />
        <activity android:name=".Acivities.MainActivity" />
        <activity android:name=".Acivities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>